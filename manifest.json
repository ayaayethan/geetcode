{
    "name": "GitCode",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Automatically pushes your leetcode solutions to GitHub.",
    "permissions": ["identity", "storage", "activeTab"],
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "https://api.github.com/*",
        "http://localhost:3000/*"
      ],
    "oauth2": {
        "client_id": "Ov23liqIyYrd2yZQOIIZ",
        "scopes": ["read:user", "repo"]
    },
    "background": {
        "service_worker": "background.js"
    }
}